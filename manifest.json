{
  "manifest_version": 3,
  "name": "YandexTracker Mobile",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icons/icon48.png",
      "96": "icons/icon96.png"
    }
  },
  "permissions": ["storage", "activeTab"],
  "icons": {
    "128": "icons/icon128.png"
  },
  "content_scripts": [{
    "matches": ["*://*.yandex.ru/*"],
    "js": ["content.js"],
    "css": ["content.css"]
  }]
}
